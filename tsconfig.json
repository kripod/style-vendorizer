{
	"compilerOptions": {
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error",
		"target": "ES5",
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"downlevelIteration": true,
		"declaration": true,
		"declarationMap": true
	}
}
