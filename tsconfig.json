{
	"extends": "@tsconfig/strictest/tsconfig.json",
	"compilerOptions": {
		"target": "ES5",
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"isolatedModules": false,
		"downlevelIteration": true,
		"declaration": true,
		"declarationMap": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	}
}
